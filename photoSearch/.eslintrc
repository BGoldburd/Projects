
{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },

    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-unused-vars": "warn",
        "no-cond-assign": ["error", "always"],
        "impliedStrict": false,
        "ecmaVersion": 6,
        "no-console": "off"
    },
    "globals": {
        "console": true,
        "document": true
        
    }
}